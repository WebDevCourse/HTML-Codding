<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="style/Main-navigation.css">
</head>
<body>
    {{#aside-navigation}}
        {{#main-navigation}}
        <nav class="Main-navigation">
            <a class="Main-navigation__item" href="{{url}}"><img class="Main-navigation__icon" src="{{icon}}"/> <span class="Aside__main-navigation-title">{{title}}</span></a>
        </nav>
        {{/main-navigation}}
        {{#aside-section}}
            <nav class="Aside__section">
                <h6 class="Aside__section-header">{{section-name}}</h6>
                {{#item}}
                    <a class="Aside__section-item" href="{{url}}"><img class="Aside__section-icon" src="{{icon}}"> <span class="Aside__section-title">{{title}}</span></a>
                {{/item}}
            </nav>
        {{/aside-section}}
    {{/aside-navigation}}
</body>
</html>